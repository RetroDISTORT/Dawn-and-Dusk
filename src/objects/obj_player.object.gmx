//check if instance was moved
sprite_index=spr_push_square image_speed=.5 v_animation=1}}}}}
//up
if keyboard_check_pressed(vk_up) and place_snapped(32,32) then{
if v_animation=0{
image_angle=90
if position_meeting(x,y-32,obj_mainblock) = false  then{
instance_create(x,y,obj_track) y=y-32}
else{
if instance_exists(obj_movable) then{
if instance_nearest(x,y-32,obj_movable).y=y-32 then if instance_nearest(x,y-32,obj_movable).x=x then if collision_rectangle(x+15,y-49,x-16,y-78,obj_mainblock,1,1) or collision_rectangle(x+15,y-49,x-16,y-78,obj_int_vert,1,1) or collision_rectangle(x+15,y-49,x-16,y-78,obj_int_hor,1,1) then{} else{
