//check if instance was moved
sprite_index=spr_push_square image_speed=.5 v_animation=1}}}}}
//left
if keyboard_check_pressed(vk_left) and place_snapped(32,32) then{
if v_animation=0{
image_angle=180
if position_meeting(x-32,y,obj_mainblock)=false  then{
instance_create(x,y,obj_track)x=x-32 }
else{
if instance_exists(obj_movable) then{
if instance_nearest(x-32,y,obj_movable).y=y then if instance_nearest(x-32,y,obj_movable).x=x-32 then if collision_rectangle(x-49,y-16,x-80,y+15,obj_mainblock,1,1) or collision_rectangle(x-49,y-16,x-80,y+15,obj_int_vert,1,1) or collision_rectangle(x-49,y-16,x-80,y+15,obj_int_hor,1,1) then{} else{
with instance_nearest(x-32,y,obj_movable){x=x-32 visible=0}
//check if instance was moved
sprite_index=spr_push_square image_speed=.5 v_animation=1}}}}}}
//zoom and focus
if obj_var_saver.v_zoom=1{
if view_wview[0]&gt;533 then{view_wview[0]-=view_wview[0]/50 view_vspeed[0]=-1}
if view_hview[0]&gt;320 then{view_hview[0]-=view_hview[0]/50 view_hspeed[0]=-1}}
else{
if view_wview[0]&lt;799 then{view_wview[0]+=view_wview[0]/50 view_vspeed[0]=-1}
if view_hview[0]&lt;479 then{view_hview[0]+=view_hview[0]/50 view_hspeed[0]=-1}}
//set fix speed and size
if obj_var_saver.v_zoom=0{
if view_wview[0]&gt;800 then{ view_wview[0]=800 view_vspeed[0]=5 view_hspeed[0]=5}
if view_hview[0]&gt;480 then{ view_hview[0]=480 view_vspeed[0]=5 view_hspeed[0]=5}}
if obj_var_saver.v_zoom=1{
if view_wview[0]&lt;533 then{ view_wview[0]=533 view_vspeed[0]=5 view_hspeed[0]=5}
if view_hview[0]&lt;320 then{ view_hview[0]=320 view_vspeed[0]=5 view_hspeed[0]=5}}
//audio listener
audio_listener_position(x,y,0)
//fake block alpha and hidden mode
if instance_exists(obj_fake_block) {
    if (v_hidden==0){
        if (distance_to_object(instance_nearest(x,y,obj_fake_block))&lt;1) 
            v_hidden=1;
        else
        if (distance_to_object(instance_nearest(x,y,obj_fake_block))&lt;16){
            last_x=x; 
            last_y=y;
        }
    }
    else {
        if distance_to_object(instance_nearest(x,y,obj_fake_block))&gt;0 {
            obj_fake_block.alarm[0]=1; 
            v_hidden=0;
        }
    }
}
    
